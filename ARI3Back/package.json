{
  "name": "nodeprojectstart",
  "version": "1.0.0",
  "description": "NodeJS based project template for easy starting new projects",
  "main": "main.js",
  "author": "Jan Johansen",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"tsc -w\" \"nodemon --inspect ./dist/main.js\" \"copy-newer --cwd src/ **.js dist/\"",
    "test": "jest --watchAll"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/ws": "^6.0.1",
    "concurrently": "^4.0.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "path": "^0.12.7",
    "rxjs": "^6.3.3",
    "ws": "^6.1.2"
  }
}
