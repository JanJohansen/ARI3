{
  "name": "nodeprojectstart",
  "version": "1.0.0",
  "description": "NodeJS based project template for easy starting new projects",
  "main": "main.js",
  "author": "Jan Johansen",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"tsc -w\" \"nodemon --inspect ./dist/main.js\" \"copy-newer -v --cwd src/ **.js dist/\" \"cpx \"../ARI3Front/src/common/*.ts\" src/common/ --watch -v\"",
    "compile": "concurrently \"tsc -w\" \"copy-newer --cwd src/ **.js dist/\" \"cpx \"../ARI3Front/src/common/*.ts\" src/common/ --watch -v\"",
    "test": "jest --watchAll"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "@types/ws": "^6.0.1",
    "concurrently": "^4.1.0",
    "copy-newer": "^2.1.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "cpx": "^1.5.0",
    "cuid": "^2.1.6",
    "eventemitter2": "^5.0.1",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "mqtt-connection": "^4.0.0",
    "node-fauxmo": "0.0.7",
    "path": "^0.12.7",
    "rxjs": "^6.5.2",
    "ws": "^7.4.5"
  }
}
